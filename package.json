{
  "name": "seedproject",
  "version": "0.0.0",
  "author": "Pyae Phyo",
  "license": "MIT",
  "scripts": {
    "start": "node ./bin/www",
    "build": "del-cli 'public/js/app' 'client/src/app/*.ngsummary.json' 'client/src/app/*.ngstyle.ts' 'client/src/app/environments/*.ngsummary.json' 'e2e/*.ngsummary.json' && webpack --config webpack.config.dev.js --progress --profile --watch",
    "build:prod": "del-cli 'public/js/app' && ngc -p tsconfig.aot.json && ngc -p tsconfig.aot.json && webpack --config webpack.config.prod.js --progress --profile --bail && del-cli 'public/js/app/**/*.js' 'public/js/app/**/*.js.map' '!public/js/app/bundle.js' '!public/js/app/*.chunk.js' 'client/src/app/**/*.ngfactory.ts' 'client/src/app/**/*.shim.ts'",
    "ng": "ng",
    "lint": "tslint \"src/**/*.ts\" --project src/tsconfig.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "mocha": "./node_modules/mocha/bin/_mocha"
  },
  "engines": {
    "node": "6.9.5",
    "npm": "3.10.10"
  },
  "dependencies": {
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/core": "^2.3.1",
    "@angular/forms": "^2.3.1",
    "@angular/http": "^2.3.1",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/platform-server": "^2.3.1",
    "@angular/router": "^3.3.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "~1.15.1",
    "cookie-parser": "~1.4.3",
    "core-js": "^2.4.1",
    "cors": "^2.8.1",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "express-validator": "^3.1.2",
    "hbs": "~4.0.0",
    "mongoose": "^4.8.3",
    "mongoose-unique-validator": "^1.0.3",
    "morgan": "~1.7.0",
    "rxjs": "^5.0.1",
    "serve-favicon": "~2.3.0",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.3.1",
    "@types/core-js": "^0.9.34",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.45",
    "angular-cli": "1.0.0-beta.26",
    "angular2-router-loader": "^0.3.2",
    "angular2-template-loader": "^0.5.0",
    "awesome-typescript-loader": "^2.2.4",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "codelyzer": "~2.0.0-beta.1",
    "del-cli": "^0.2.1",
    "html-loader": "^0.4.4",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "karma-spec-reporter": "0.0.26",
    "mocha": "^3.2.0",
    "protractor": "~4.0.13",
    "raw-loader": "^0.5.1",
    "ts-node": "1.2.1",
    "tslint": "^4.3.0",
    "typescript": "~2.0.3",
    "webpack": "^2.2.0-rc.0",
    "webpack-merge": "^0.14.1"
  }
}
